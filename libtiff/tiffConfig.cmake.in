# - Config file for the tiff package
# It defines the following variables
#  TIFF_INCLUDE_DIR - include directory
#  TIFF_LIBRARIES   - libraries to link against

# Compute paths
get_filename_component(TIFF_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)
set(TIFF_INCLUDE_DIR "@CONFIG_INCLUDE_DIRS@")

# Our library dependencies (contains definitions for IMPORTED targets)
include("${TIFF_CMAKE_DIR}/tiffTargets.cmake")

# These are IMPORTED targets created by yaml-cpp-targets.cmake
set(TIFF_LIBRARIES "@EXPORT_TARGETS@")